    :price "30.0"^^xsd:float .

:Beach a owl:Class ;
    rdfs:label "Beach"@en ;
    rdfs:comment "A sandy or pebbly shore"@en ;
    rdfs:subClassOf :NaturalSite .

:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "A scheduled happening or festival"@en ;
    rdfs:subClassOf :Activity .

:Monument a owl:Class ;
    rdfs:label "Monument"@en ;
    rdfs:comment "A structure of historical or cultural significance"@en ;
    rdfs:subClassOf :TouristAttraction .

:Museum a owl:Class ;
    rdfs:label "Museum"@en ;
    rdfs:comment "A building housing artifacts and exhibitions"@en ;
    rdfs:subClassOf :TouristAttraction .

:NaturalSite a owl:Class ;
    rdfs:label "Natural Site"@en ;
    rdfs:comment "A natural landmark or protected area"@en ;
    rdfs:subClassOf :TouristAttraction .

:Tour a owl:Class ;
    rdfs:label "Tour"@en ;
    rdfs:comment "A guided visit to places of interest"@en ;
    rdfs:subClassOf :Activity .

ex:Algeria a :Country ;
    :description "North African country with Mediterranean coastline" ;
    :name "Algeria"@en .

ex:Algiers a :City ;
    :description "Capital and largest city of Algeria" ;
    :locatedIn ex:Algeria ;
    :name "Algiers"@en .

:Country a owl:Class ;
    rdfs:label "Country"@en ;
    rdfs:comment "A nation state"@en ;
    rdfs:subClassOf :Place .

:City a owl:Class ;
    rdfs:label "City"@en ;
    rdfs:comment "An urban settlement"@en ;
    rdfs:subClassOf :Place .

:Hotel a owl:Class ;
    rdfs:label "Hotel"@en ;
    rdfs:comment "A commercial establishment providing lodging"@en ;
    rdfs:subClassOf :Accommodation .

:Restaurant a owl:Class ;
    rdfs:label "Restaurant"@en ;
    rdfs:comment "A place where meals are served"@en ;
    rdfs:subClassOf :Service .

:Service a owl:Class ;
    rdfs:label "Service"@en ;
    rdfs:comment "A service provided to tourists"@en .

ex:Oran a :City ;
    :description "Major port city in northwest Algeria" ;
    :hasActivity ex:OranCityTour,
        ex:OranFestival ;
    :locatedIn ex:Algeria ;
    :name "Oran"@en .

:TouristAttraction a owl:Class ;
    rdfs:label "Tourist Attraction"@en ;
    rdfs:comment "A place of interest for tourists"@en ;
    rdfs:subClassOf :Place .

:Activity a owl:Class ;
    rdfs:label "Activity"@en ;
    rdfs:comment "An activity or experience available to tourists"@en .

:Accommodation a owl:Class ;
    rdfs:label "Accommodation"@en ;
    rdfs:comment "A place where tourists can stay"@en .

:Place a owl:Class ;
    rdfs:label "Place"@en ;
    rdfs:comment "A geographic location or point of interest"@en .

