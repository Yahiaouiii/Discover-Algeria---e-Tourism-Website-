<http://www.semanticweb.org/ontologies/etourism> a owl:Ontology ;
    rdfs:label "E-Tourism Ontology"@en ;
    rdfs:comment "An ontology for modeling tourism-related entities, places, accommodations, and activities"@en .

:Experience a owl:Class ;
    rdfs:label "Experience"@en ;
    rdfs:comment "An immersive tourist activity"@en ;
    rdfs:subClassOf :Activity .

:Hostel a owl:Class ;
    rdfs:label "Hostel"@en ;
    rdfs:comment "Budget accommodation with shared facilities"@en ;
    rdfs:subClassOf :Accommodation .

:Resort a owl:Class ;
    rdfs:label "Resort"@en ;
    rdfs:comment "A full-service accommodation with recreational facilities"@en ;
    rdfs:subClassOf :Accommodation .

:Transportation a owl:Class ;
    rdfs:label "Transportation"@en ;
    rdfs:comment "A means of moving tourists"@en ;
    rdfs:subClassOf :Service .

:address a owl:DatatypeProperty ;
    rdfs:label "address"@en ;
    rdfs:comment "Physical address"@en ;
    rdfs:domain :Place ;
    rdfs:range xsd:string .

:capacity a owl:DatatypeProperty ;
    rdfs:label "capacity"@en ;
    rdfs:comment "Maximum number of guests"@en ;
    rdfs:domain :Accommodation ;
    rdfs:range xsd:integer .

:description a owl:DatatypeProperty ;
    rdfs:label "description"@en ;
    rdfs:comment "A textual description"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

:duration a owl:DatatypeProperty ;
    rdfs:label "duration"@en ;
    rdfs:comment "Duration in minutes"@en ;
    rdfs:domain :Activity ;
    rdfs:range xsd:integer .

:hasActivity a owl:ObjectProperty ;
    rdfs:label "has activity"@en ;
    rdfs:comment "Indicates available activities at a place"@en ;
    rdfs:domain :Place ;
    rdfs:range :Activity .

:hasEmail a owl:DatatypeProperty ;
    rdfs:label "email"@en ;
    rdfs:comment "Email contact"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

:hasPhone a owl:DatatypeProperty ;
    rdfs:label "phone"@en ;
    rdfs:comment "Phone contact"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

:locatedIn a owl:ObjectProperty ;
    rdfs:label "located in"@en ;
    rdfs:comment "Indicates that a place is within another place"@en ;
    rdfs:domain :Place ;
    rdfs:range :Place .

:name a owl:DatatypeProperty ;
    rdfs:label "name"@en ;
    rdfs:comment "The name of an entity"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

:nearTo a owl:ObjectProperty ;
    rdfs:label "near to"@en ;
    rdfs:comment "Indicates proximity between places"@en ;
    rdfs:domain :Place ;
    rdfs:range :Place .

:offers a owl:ObjectProperty ;
    rdfs:label "offers"@en ;
    rdfs:comment "Indicates that an accommodation provides a service"@en ;
    rdfs:domain :Accommodation ;
    rdfs:range :Service .

:openingHours a owl:DatatypeProperty ;
    rdfs:label "opening hours"@en ;
    rdfs:comment "Operating hours"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :TouristAttraction :Restaurant ) ] ;
    rdfs:range xsd:string .

:price a owl:DatatypeProperty ;
    rdfs:label "price"@en ;
    rdfs:comment "Price in currency units"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Activity :Accommodation ) ] ;
    rdfs:range xsd:float .

:priceRange a owl:DatatypeProperty ;
    rdfs:label "price range"@en ;
    rdfs:comment "Price category (e.g., $, $$, $$$)"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Accommodation :Restaurant ) ] ;
    rdfs:range xsd:string .

:providedBy a owl:ObjectProperty ;
    rdfs:label "provided by"@en ;
    rdfs:comment "Indicates which service provider offers an activity"@en ;
    rdfs:domain :Activity ;
    rdfs:range :Service .

:rating a owl:DatatypeProperty ;
    rdfs:label "rating"@en ;
    rdfs:comment "Rating score (0-5)"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Accommodation :Restaurant :TouristAttraction ) ] ;
    rdfs:range xsd:float .

